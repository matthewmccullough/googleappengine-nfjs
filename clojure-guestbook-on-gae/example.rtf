\
(ns compojure.http\
  "Shortcut to include compojure.http.routes, compojure.http.helpers and\
   compojure.http.servlet."\
  (:use compojure.ns-utils))\
\
(immigrate\
  'compojure.http.helpers\
  'compojure.http.middleware\
  'compojure.http.routes\
  'compojure.http.servlet)\

