<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <welcome-file-list>
    <welcome-file>input.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>sudokuApi</servlet-name>
    <servlet-class>com.google.appengine.demos.gwtdlx.server.SudokuApiImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>sudokuApi</servlet-name>
    <url-pattern>/gwtdlx/solve</url-pattern>
  </servlet-mapping>
</web-app>